    <declare-styleable name="Ratio"><attr format="float" name="ratio"/><attr format="boolean" name="baseOnWidth"/><attr format="boolean" name="matchDrawableRatio"/></declare-styleable>
    <declare-styleable name="Search_View"><attr format="dimension" name="textSizeSearch"/><attr format="color" name="textColorSearch"/><attr format="string" name="textHintSearch"/><attr format="integer" name="searchBlockHeight"/><attr format="color" name="searchBlockColor"/></declare-styleable>
    <declare-styleable name="SimpleGridLayout"><attr format="integer" name="grid_column"/><attr format="dimension" name="grid_row_height"/><attr format="float" name="grid_item_height_width_ratio"/><attr format="dimension" name="grid_divider_width"/></declare-styleable>
