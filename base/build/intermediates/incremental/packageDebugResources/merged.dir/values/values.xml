        </attr><attr format="dimension" name="radius"/><attr format="boolean" name="snap"/><attr format="color" name="strokeColor"/><attr format="dimension" name="strokeWidth"/></declare-styleable>
    <declare-styleable name="ClearEditText"><attr format="reference" name="clear_drawable"/><attr format="boolean" name="clear_phone"/></declare-styleable>
    <declare-styleable name="FormLayout"><attr format="float" name="relative_width"/><attr format="float" name="relative_height"/><attr format="float" name="relative_left"/><attr format="float" name="relative_top"/><attr format="float" name="w_h_ratio"/><attr format="boolean" name="base_on_width"/></declare-styleable>
