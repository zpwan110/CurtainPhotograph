<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- outer circle -->
    <item android:left="-2dp"
        android:top="-2dp"
        android:right="-2dp">
        <shape android:shape="rectangle" >
            <!-- 描边 -->
            <stroke
                android:width="1dp"
                android:color="#f2f2f5"
                android:dashWidth="5dp"
                android:dashGap="0dp"
                />
        </shape>
    </item>
</layer-list>