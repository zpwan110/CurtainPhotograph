<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- outer circle -->
    <item >
        <shape android:shape="rectangle" >
            <!-- 描边 -->
            <stroke
                android:width="1dp"
                android:color="#2d2d2d"
                android:dashWidth="5dp"
                android:dashGap="0dp"
                />
            <corners android:radius="5dp"/>
        </shape>
    </item>
</layer-list>
